# HELP exim_processes Number of running exim process broken down by state (delivering, handling, etc)
# TYPE exim_processes gauge
exim_processes{state="daemon"} 1
exim_processes{state="delivering"} 2
exim_processes{state="handling"} 3
exim_processes{state="other"} 2
exim_processes{state="running"} 1
# HELP exim_messages_total Total number of logged messages broken down by flag (delivered, deferred, etc)
# TYPE exim_messages_total counter
exim_messages_total{flag="additional",router="",transport=""} 1
exim_messages_total{flag="arrived",router="",transport=""} 8
exim_messages_total{flag="completed",router="",transport=""} 4
exim_messages_total{flag="deferred",router="dnslookup",transport="remote_smtp"} 1
exim_messages_total{flag="delivered",router="dnslookup",transport="remote_smtp"} 2
exim_messages_total{flag="delivered",router="dnslookup",transport="remote_smtp_smarthost"} 1
exim_messages_total{flag="failed",router="",transport=""} 1
exim_messages_total{flag="failed",router="dnslookup",transport="remote_smtp"} 3
# HELP exim_panic_total Total number of logged panic messages
# TYPE exim_panic_total counter
exim_panic_total 6
# HELP exim_queue Number of messages currently in queue
# TYPE exim_queue gauge
exim_queue 126
# HELP exim_reject_total Total number of logged reject messages
# TYPE exim_reject_total counter
exim_reject_total{reason="535 Incorrect authentication data"} 1
exim_reject_total{reason="SPF check failed"} 1
exim_reject_total{reason="other"} 4
exim_reject_total{reason="relay not permitted"} 1
# HELP exim_up Whether or not the main exim daemon is running
# TYPE exim_up gauge
exim_up 1
